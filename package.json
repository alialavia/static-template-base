{
  "name": "templated_bootstrap_modern_business",
  "version": "0.1.0",
  "description": "This is a starter for working with [gulp-static-template-resolver](https://github.com/alialavia/gulp-static-template-resolver) in order to generate the htmls based on some json data and javascript commands, stored in src/templates/resolve.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js"
  },
  "author": "Ali Alavi",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
