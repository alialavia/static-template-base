    "gulp-cssnano": "^2.1.2",
    "gulp-es6-template-strings": "^1.0.1",
    "gulp-ignore-errors": "0.0.4",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^3.1.0",
    "gulp-static-template-resolver": "^0.1.0",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.4",
    "require-reload": "^0.2.2",
    "run-sequence": "^2.2.1",
    "zip-array": "^1.0.1"
  },
  "dependencies": {
    "npm": "^6.0.1"
  },
  "repository": {
    "type": "git",
